#include <bits/stdc++.h>
 using namespace std; 
int main() {  
long long n;  
cin>>n;  
long long cnt = 0; 
 for (int i = 1; i*i <= n; i++) {  
           if (n % i==0) {    
                    cnt++;   
           if (i*i != n)    
                    cnt++; 
           } 
}  
cout<<cnt; 
 return 0; 
}
// 10^9 uchun boÊ»luvchilar soni
